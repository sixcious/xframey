{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "short_name": "__MSG_short_name__",
  "description": "__MSG_description__",
  "version": "0.0",
  "default_locale": "en",
  "icons": { "16": "img/icon.png", "48": "img/icon.png", "128": "img/icon.png" },
  "browser_action": { "default_title": "__MSG_title__", "default_icon": { "16": "img/icon.png", "24": "img/icon.png", "32": "img/icon.png" }, "default_popup": "html/popup.html" },
  "background": { "scripts": ["js/promisify.js", "js/background.js"], "persistent": true },
  "permissions": ["storage", "webRequest", "webRequestBlocking", "<all_urls>"],
  "minimum_chrome_version": "60"
}
